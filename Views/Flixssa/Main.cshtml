@model List<Filmes.Models.Filme> 

<h1>@ViewBag.Title</h1>
<table class="table">

    <tr>
        <th>Titulo</th>
        <th>Lancamento</th>
        <td>Type</td>
    </tr>
  @foreach (var filme in Model)
    {
        <tr>
            <td>@filme.Titulo</td>
            <td>@filme.Lancamento</td>
            <td>@filme.Type</td>
        </tr>
    }
</table>
@if (Model.Count == 0)
{
    <p>No Filmes yet!</p>
}

<p><a asp-controller="Flixssa" asp-action="Add">Add Filme</a></p>

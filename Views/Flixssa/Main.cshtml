@model List<Filmes.Models.Filme> 

<h1>@ViewBag.Title</h1>
<table class="table">

    <tr>
        <th>Name</th>
        <th>Description</th>
        <td>Type</td>
    </tr>
  @foreach (var Filme in Model)
    {
        <tr>
            <td>@Filme.Titulo</td>
            <td>@Filme.Lancamento</td>
            <td>@Filme.Type</td>
        </tr>
    }
</table>
@if (Model.Count == 0)
{
    <p>No Filmes yet!</p>
}

<p><a asp-controller="Filme" asp-action="Add">Add Filme</a></p>
